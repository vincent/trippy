    "npm": "2.x || 3.x"
  },
  "contributors": [
    "Vincent Lark (https://github.com/vincent)"
  ],
  "build": {
    "appId": "vlark.trippy",
    "category": "public.app-category.games",
    "productName": "Trippy",
    "compression": "normal",
    "asar": true,
    "mac": {
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file",
          "path": "Trippy.app"
        }
      ]
    }
  },
  "directories": {
    "app": "./dist/",
    "output": "./release/"
  }
}
