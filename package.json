    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.23.1",
    "classnames": "^2.2.5",
    "concurrently": "^2.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.24.0",
    "css-modules-require-hook": "^4.0.0",
    "del": "^2.2.2",
